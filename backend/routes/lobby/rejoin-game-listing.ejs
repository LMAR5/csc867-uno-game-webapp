<template id="rejoin-game-template">
    <%- include("rejoin-game", { game: { id: -1, gravatar: "", creator_email: "@",}}) %>
  </template>
  <ul
    role="list"
    id="rejoin-games-list"
    class="mt-3 grid grid-cols-1 gap-5 sm:grid-cols-1 sm:gap-6 lg:grid-cols-1"
  >
    <% rejoinGames.forEach(game => { %>
    <%- include("rejoin-game", { game }) %>
    <% }) %>
  </ul>
  